{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "quotes": ["error", "single"]
    },
    "plugins": [ "html" ]
}
